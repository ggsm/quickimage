<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
 
     <extension
         point="org.eclipse.ui.editors">
      <editor
            class="nu.psnet.quickimage.editors.QuickImageEditor"
            icon="icons/picture.png"
            default="true"
            name="QuickImage Editor"
            id="nu.psnet.quickimage.editors.QuickImageEditor"
            extensions="gif,jpg,jpeg,png,ico,bmp">
      </editor>

      <editor
            class="nu.psnet.quickimage.editors.QuickImageEditor"
            icon="icons/picture_affinity.png"
            default="true"
            name="QuickImage Editor"
            id="nu.psnet.quickimage.editors.QuickImageEditorAffinity"
            extensions="afdesign">
      </editor>

   </extension>

</plugin>
